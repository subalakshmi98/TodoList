<%- include("header") -%>


    <div class="box" id="heading">
     <h1><%= listTitle %> </h1>
    </div>

    <div class="box">
      <% for (var i = 0; i < addname.length; i++) { %>
      <div class="item">
         <input type="checkbox">
            <p> <%=addname[i] %> </p>
      </div>
      <% } %>

     <form class="item" action="/" method="post">
       <input type="text" name="listname" placeholder="New Items" autocomplete="off">
       <button type="submit" name="list" value=<%= listTitle %>>+</button>
     </form>
     </div>

<%- include("footer") -%>
